{
  "name": "webapp-core",
  "version": "0.0.1",
  "description": "core react isomorphic webapp",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js bin config src test webpack",
    "lint:gulp": "gulp lint",
    "test": "mocha --compilers js:babel-register --require test/test-helper.js test/**/*.test.js",
    "test:gulp": "gulp test",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter text --reporter text-summary --reporter html npm test",
    "test:coverage:gulp": "cross-env NODE_ENV=test gulp test:coverage",
    "watch": "nodemon -e js -x 'npm run lint && npm run test'",
    "watch:coverage": "nodemon -e js -x 'npm run lint && npm run test:coverage'",
    "start:dev": "./bin/server.js",
    "start:webpack-dev-server": "./webpack/webpack-dev-server.js",
    "dev": "concurrently -r --kill-others 'npm run start:webpack-dev-server' 'npm run start:dev'",
    "dev:watch": "concurrently -r --kill-others 'npm run watch' 'npm run start:webpack-dev-server' 'npm run start:dev'",
    "dev:watch:coverage": "concurrently -r --kill-others 'npm run watch:coverage' 'npm run start:webpack-dev-server' 'npm run start:dev'"
  },
  "keywords": [
    "react",
    "isomorphic",
    "webapp",
    "server"
  ],
  "author": "Patrick Fitzgerald <auskast03@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.6 <7"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "compression": "^1.6.2",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-babel-istanbul": "^1.5.0",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-sequence": "^0.4.6",
    "jsdom": "^9.5.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-isomorphic-tools": "^2.5.8"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "nyc": "^8.3.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.3.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  }
}
